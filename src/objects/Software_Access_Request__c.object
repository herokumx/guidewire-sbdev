    <validationRules>
        <fullName>Request_Type_required</fullName>
        <active>true</active>
        <errorConditionFormula>IF(ISPICKVAL(Request_type__c, &apos;&apos;) &amp;&amp;  ISPICKVAL(Status__c, &apos;Submitted&apos;) , TRUE, FALSE)</errorConditionFormula>
        <errorDisplayField>Request_type__c</errorDisplayField>
        <errorMessage>Request Type is Required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Thunderhead_End_Date_is_required</fullName>
        <active>true</active>
        <errorConditionFormula>INCLUDES(Products__c, &quot;Thunderhead&quot;) &amp;&amp; ISBLANK( Thunderhead_End_Date__c) &amp;&amp; ISPICKVAL(Status__c , &apos;Submitted&apos;)</errorConditionFormula>
        <errorDisplayField>Thunderhead_End_Date__c</errorDisplayField>
        <errorMessage>Thunderhead End Date is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Date_Needed</fullName>
        <active>true</active>
        <errorConditionFormula>IF( ISBLANK(Date_needed__c) &amp;&amp;  ISPICKVAL(Status__c , &apos;Submitted&apos;), TRUE , FALSE)</errorConditionFormula>
        <errorDisplayField>Date_needed__c</errorDisplayField>
        <errorMessage>Date needed is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Products</fullName>
        <active>true</active>
        <errorConditionFormula>IF( ISBLANK(Products__c) &amp;&amp;  ISPICKVAL(Status__c , &apos;Submitted&apos;), TRUE , FALSE)</errorConditionFormula>
        <errorDisplayField>Products__c</errorDisplayField>
        <errorMessage>Please select at least one product.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Service_standard</fullName>
        <active>true</active>
        <errorConditionFormula>IF( ISPICKVAL(Request_type__c, &apos;New customer&apos;) &amp;&amp;  ISPICKVAL(Status__c , &apos;Submitted&apos;) &amp;&amp; ISPICKVAL( Service_standard__c, &apos;&apos;), TRUE , FALSE)</errorConditionFormula>
        <errorDisplayField>Service_standard__c</errorDisplayField>
        <errorMessage>Service standard is required if Request Type is New Customer.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Software_Removal_Date</fullName>
        <active>true</active>
        <errorConditionFormula>IF( ISBLANK(Software_Removal_Date__c) &amp;&amp; ( ISPICKVAL(Request_type__c , &apos;Temporary prospect&apos;)  || ISPICKVAL(Request_type__c , &apos;Early Adopters&apos;)) &amp;&amp;  ISPICKVAL(Status__c , &apos;Submitted&apos;), TRUE , FALSE)</errorConditionFormula>
        <errorDisplayField>Software_Removal_Date__c</errorDisplayField>
        <errorMessage>Software Removal Date is required.</errorMessage>
    </validationRules>
</CustomObject>
