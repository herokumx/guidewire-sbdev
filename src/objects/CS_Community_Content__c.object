    <webLinks>
        <fullName>Activate</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Activate</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)}
var tLocation=&quot;{!CS_Community_Content__c.CS_Location__c}&quot;;
var tCommunity=&quot;{!CS_Community_Content__c.CS_Community__c}&quot;;
// query the record


var qr = sforce.connection.query(&quot;SELECT Id,Name,CS_Status__c FROM CS_Community_Content__c where CS_status__c=&apos;Active&apos; and CS_Location__c=&apos;tLocation&apos; and CS_Community__c=&apos;tCommunity&apos;  &quot; );

var records = qr.getArray(&quot;records&quot;);


if (records.length &gt; 0)
{

  if(qr.records.CS_Location__c ==&apos;{!CS_Community_Content__c.CS_Location__c}&apos;)
  {
      alert(&quot;This Content is already activated&quot;);
  }
else
  {
     alert(&quot;There can be only 1 active content for a location at a time. Content &quot; + qr.records.Name + &quot; is already active for Location {!CS_Community_Content__c.CS_Location__c}&quot; );
  }
}
else
{
var o = new sforce.SObject(&quot;CS_Community_Content__c&quot;);
o.id = &quot;{!CS_Community_Content__c.Id}&quot;;
 
// make the field change
o.CS_Status__c = &quot;Active&quot;;
 
// save the change
sforce.connection.update([o]);
//refresh the page
window.location.reload();
}</url>
    </webLinks>
